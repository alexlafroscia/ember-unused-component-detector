{
  "name": "ember-unused-component-detector",
  "version": "1.0.0",
  "description": "Locate Ember components that are not actually used in your application",
  "author": "Alex LaFroscia <alex@lafroscia.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "test": "jest",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "ripgrep-js": "^2.2.0"
  },
  "devDependencies": {
    "@movable/eslint-config": "^1.0.0",
    "@movable/eslint-config-node": "^1.0.0",
    "@movable/eslint-config-typescript": "^1.0.0",
    "@movable/prettier-config": "^1.0.0",
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.20",
    "eslint": "^7.26.0",
    "file-fixture-factory": "^1.0.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.4",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "volta": {
    "node": "14.15.5",
    "yarn": "1.22.10"
  }
